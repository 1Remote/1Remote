<Application x:Class="PRM.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="App_OnStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/PRM.Core;component/Languages/en-us.xaml"></ResourceDictionary>
                <ResourceDictionary Source="./Resources/Converter/Converter.xaml"></ResourceDictionary>
                <ResourceDictionary Source="./Resources/Theme/DefaultTheme.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>